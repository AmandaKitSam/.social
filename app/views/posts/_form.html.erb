<h1>Post</h1>

<%= form_for @post, html: {multipart: true} do |form| %>


  <div class="field">
  <%= form.label :dateOfPost %>
  <%= form.date :dateOfPost %>
  </div>

  <div class="field">
  <%= form.label :user_id %>
  <%= form.file_field :user_id %>
  </div>

  <div class="field">
  <%= form.label :reply %>
  <%= form.text_area :reply %>
  </div>

  <div class="actions">
  <%= form.submit %>
  </div>
<% end %>
