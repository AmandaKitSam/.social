<h1>Edit Photos</h1>

<%= form_for @photo, :html => {:multipart => true} do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <p>Add an URL link:</p>
  <%= f.label :image %>
  <%= f.text_field :image %>

  <p>Add an URL link:</p>
  <%= f.label :image %>
  <%= f.file_field :image %>


  <%= f.label :comment %>
  <%= f.text_field :comment, :required => true %>

  <%= f.submit %>

  <% end %>
